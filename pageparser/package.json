{
  "name": "pageparser",
  "version": "0.0.0a",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "codestyle" : "node ./node_modules/jshint/bin/jshint ./public/javascripts/*.js ./bin ./tests/*.js",
    "test": "node ./node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=tests/jasmine.json",
    "verify" : "npm run-script codestyle && npm run-script test"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "request": "^2.69.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "jasmine": "^2.4.1",
    "jshint": "^2.9.1"
  }
}
